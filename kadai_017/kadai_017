class Human:
  def __init__(self, name, age):
    self.name = name
    self.age = age

  def check_adult(self):
    print(f"名前は{self.name}です")

    if self.age >= 20:
      print(f"年齢は{self.age}歳なので大人です。")
    else:
      print(f"年齢は{self.age}歳なので子供です。")

humans = [
    Human("田中", 20),
    Human("佐藤", 33),
    Human("高橋", 15),
    Human("阿部", 5),
]

for human in humans:
  human.check_adult()
